                                android:src="@drawable/atstation" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow20"
                                android:layout_gravity="right"
                                android:src="@drawable/rightarrow" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation20"
                                android:layout_gravity="right"
                                android:src="@drawable/atstation" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow21"
                                android:layout_gravity="right"
                                android:src="@drawable/rightarrow" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation21"
                                android:layout_gravity="right"
                                android:src="@drawable/atstation" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                </FrameLayout>
            </HorizontalScrollView>
        </ScrollView>
    </LinearLayout>

</RelativeLayout>
