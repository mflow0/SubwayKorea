                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:weightSum="1"
                        android:layout_weight="0.20" />

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:weightSum="1"
                        android:layout_weight="0.09" >

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.14" />

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="0.5" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow1"
                                android:src="@drawable/downarrow"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation1"
                                android:src="@drawable/atstation"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow2"
                                android:src="@drawable/downarrow"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation2"
                                android:src="@drawable/atstation"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow3"
                                android:src="@drawable/downarrow"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation3"
                                android:src="@drawable/atstation"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow4"
                                android:src="@drawable/downarrow"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation4"
                                android:src="@drawable/atstation"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow5"
                                android:src="@drawable/downarrow"
                                android:layout_gravity="right" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation5"
                                android:src="@drawable/atstation"
                                android:layout_gravity="right" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:weightSum="1"
                        android:layout_weight="0.07" >

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_gravity="right"
                            android:layout_weight="0.67" />

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_weight="0.03" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow6"
                                android:layout_gravity="right"
                                android:src="@drawable/rightarrow" />

                            <ImageView
