                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow16"
                                android:layout_gravity="right"
                                android:src="@drawable/rightarrow" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation16"
                                android:layout_gravity="right"
                                android:src="@drawable/atstation" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengArrow17"
                                android:layout_gravity="right"
                                android:src="@drawable/rightarrow" />

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:id="@+id/sanghengStation17"
                                android:layout_gravity="right"
